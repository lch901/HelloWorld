package dao;

import java.sql.Connection;
import java.sql.PreparedStatement;

public class MemberDAO {
	public void insertMember(){
		Connection con=null;
		PreparedStatement ps=null;
		
		try {
			con=DAO.getCon();
			ps=con.prepareStatement(" insert into t_member(i_member,mid,mpw,nm,sex)values((select nvl(max(i_member),0)+1,?,?,?,?) ");
		}catch(Exception e) {
			e.printStackTrace();
		}
	}
}
